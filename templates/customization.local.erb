[<%= @name %>]
enabled = <%= @enabled %>
<% if @port -%>
port    = <%= @port.join(',') %>
<% end -%>
<% if @logpath -%>
logpath = <%= @logpath %>
<% end -%>
<% if @backend -%>
backend = <%= @backend %>
<% end -%>
<% if @bantime -%>
bantime = <%= @bantime %>
<% end -%>
<% if @maxretry -%>
maxretry = <%= @maxretry %>
<% end -%>
<% if @filter -%>
filter = <%= @filter %>
<% end -%>
<% if @action -%>
action = <%= @action %>
<% end -%>
